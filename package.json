  "repository": {
    "type": "git",
    "url": "https://github.com/joanllenas/ts.data.json"
  },
  "bugs": {
    "url": "https://github.com/joanllenas/ts.data.json/issues"
  },
  "homepage": "https://github.com/joanllenas/ts.data.json#readme",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^5.2.7",
    "chai": "^4.3.4",
    "mocha": "^6.2.3",
    "prettier": "^2.3.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.0"
  }
